{
  "name": "jada-cv",
  "version": "2.1.0",
  "description": "My CV",
  "main": "index.js",
  "scripts": {
    "start": "cd build; node index.js",
    "eslint": "eslint --fix \"src/**/*.js\" || exit 0",
    "tslint": "tslint --fix -p .",
    "tslint:force": "tslint --fix --force -p .",
    "prettier": "prettier --write \"**/*.{ts,js}\"",
    "format": "npm-run-all prettier eslint prettier"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jadaradix/jada-cv.git"
  },
  "keywords": [],
  "author": "James Garner <j@jada.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jadaradix/jada-cv/issues"
  },
  "homepage": "https://github.com/jadaradix/jada-cv",
  "dependencies": {
    "pug-cli": "^1.0.0-alpha6"
  },
  "devDependencies": {
    "http-server": "^0.11.1",
    "node-sass": "^4.13.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "lint-staged": "^7.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.16.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "postcommit": "git update-index -g"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
