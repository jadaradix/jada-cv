- var js = ["jada"]
- var css = ["jada"]
- var title = "James Garner"
doctype html
html(ng-app="jadaSite")
  head
    include partials/_head.jade
  body
    #Wrapper
      aside.tile-column
        .tile-row
          include partials/_tile-me.jade
        .tile-row
          include partials/_tile-me-text.jade
      main.tile-column
        .tile-row
          include partials/_tile-projects.jade
          include partials/_tile-cv.jade
        .tile-row
          include partials/_tile-about-me.jade
        .tile-row.spread-tall.hide#main
          include partials/_tile-main.jade
        .tile-row
          include partials/_tile-single.jade
          include partials/_tile-single.jade
        .tile-row
          include partials/_tile-double.jade